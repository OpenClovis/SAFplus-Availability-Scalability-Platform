ClRcT clAlarm_clock_EOIdlSyncDefer(ClIdlHandleT *pIdlHdl)
{
    return clIdlSyncDefer(alarm_clock_EOidlDatabaseHdl, alarm_clock_EOidlSyncKey, pIdlHdl);
}    
    
ClRcT clAlarm_clock_EOClientInstall(void)
{
    ClEoExecutionObjT* pEoObj = NULL;
    ClRcT rc = CL_OK;

    rc = clEoMyEoObjectGet(&pEoObj);
    if (CL_OK != rc)
    {
        return rc;
    }

    rc = clEoClientInstallTables(pEoObj,CL_EO_SERVER_SYM_MOD(gAspFuncTable,Alarm_clock_EO));
    if (CL_OK != rc)
    {
        return rc;
    }
    
    rc = clOsalTaskKeyCreate(&alarm_clock_EOidlSyncKey,NULL);
    if (CL_OK != rc)
    {
        return rc;
    }

    rc = clHandleDatabaseCreate(NULL,&alarm_clock_EOidlDatabaseHdl);

    return rc;
}

ClRcT clAlarm_clock_EOClientUninstall(void)
{
    ClEoExecutionObjT* pEoObj = NULL;
    ClRcT rc = CL_OK;

    rc = clEoMyEoObjectGet(&pEoObj);
    if (CL_OK != rc)
    {
        return rc;
    }
    rc = clOsalTaskKeyDelete(alarm_clock_EOidlSyncKey);
    if (CL_OK != rc)
    {
        return rc;
    }
    rc = clHandleDatabaseDestroy(alarm_clock_EOidlDatabaseHdl);
    if (CL_OK != rc)
    {
        return rc;
    }

    clEoClientUninstallTables(pEoObj,CL_EO_SERVER_SYM_MOD(gAspFuncTable,Alarm_clock_EO));

    return rc;
}

    
