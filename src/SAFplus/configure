    cross_compiling=yes;
    if [ -f $BUILD_TOOLS_DIR/$CROSS_BUILD/config.mk ]; then
        source $BUILD_TOOLS_DIR/$CROSS_BUILD/config.mk
    fi
    if [ "$WIND_VER" = "1.2" ]; then
        export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-pne-gcc
        export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-pne-g++
        LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-pne-ld
        AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-pne-ar
        RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-pne-ranlib
    elif [ "$WIND_VER" = "0" ]; then
        if [ "$BIN_MODE" = "32" ]; then
            export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-gcc -m32
            export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-g++ -m32
            LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ld
            AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ar
            RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ranlib
        elif [ "$BIN_MODE" = "64" ]; then
            export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-gcc -m64
            export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-g++ -m64
            LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ld
            AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ar
            RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ranlib
        else
            export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-gcc
            export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-g++
            LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ld
            AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ar
            RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$CPU-ranlib
        fi
    elif [ "$WIND_VER" = "2.0" ]; then
        $WIND_HOME/wrenv.sh -p wrlinux-2.0 env > wrs.env
        grep 'WIND' wrs.env > wrs.mk
        grep '^PATH' wrs.env >> wrs.mk
        grep 'LD_LIBRARY_PATH' wrs.env >> wrs.mk
        grep 'DFW' wrs.env >> wrs.mk
        grep 'WRSD' wrs.env >> wrs.mk
        sed -i -e 's/^/export /g' wrs.mk
        source wrs.mk
        rm wrs.env 
        export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-$ROOTFS-gcc
        export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-$ROOTFS-g++
        export LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-$ROOTFS-ld
        export AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-$ROOTFS-ar
        export RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-$CPU-$ROOTFS-ranlib
    else
        export CC=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-gcc
        export CXX=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-g++
        LD=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-ld
        AR=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-ar
        RANLIB=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/$TARGET-ranlib
    fi
    GCC=$CC
    export PKG_CONFIG_PATH=$BUILD_TOOLS_DIR/$CROSS_BUILD/local/lib/pkgconfig:$BUILD_TOOLS_DIR/$CROSS_BUILD/lib/pkgconfig
    if [ -f $BUILD_TOOLS_DIR/$CROSS_BUILD/local/bin/net-snmp-config ]; then
        export NET_SNMP_CONFIG=$BUILD_TOOLS_DIR/$CROSS_BUILD/local/bin/net-snmp-config
    elif [ -f $BUILD_TOOLS_DIR/$CROSS_BUILD/target/bin/net-snmp-config ]; then
