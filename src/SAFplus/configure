    else
        export NET_SNMP_CONFIG=$BUILD_TOOLS_DIR/$CROSS_BUILD/bin/net-snmp-config
    fi
    export KERNEL_SOURCE=$BUILD_TOOLS_DIR/$CROSS_BUILD/src/$KERNEL

    CL_TARGET_PLATFORM=$ARCH

    if [ -n "$BIN_MODE" ]; then
        export CL_BINMODE=$BIN_MODE
    fi

fi;

