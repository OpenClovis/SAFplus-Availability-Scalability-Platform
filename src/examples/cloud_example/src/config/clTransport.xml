<?xml version="1.0" encoding="UTF-8"?>
<openClovisAsp>
  <version v0="4.0.0">
    <ioc:BootConfig xmlns:ioc="ioc.ecore">
     <config>
       <default>TIPC</default>
     </config>
      <xports>
        <xport>
          <type>TIPC</type>
          <plugin>libClTIPC.so</plugin>
        </xport>
        <xport>
          <type>UDP</type>
          <plugin>libClUDP.so</plugin>
        </xport>
      </xports>
      <protocol default="UDP"/>
      <heartbeat>
          <interval>2000</interval>
          <intervalLocal>1000</intervalLocal>
          <retries>3</retries>
      </heartbeat>
      <peerAddresses port="6799">
         <peer addr="192.168.6.101"/>
         <!--peer addr="192.168.6.102"/-->
      </peerAddresses>
    </ioc:BootConfig>
  </version>
</openClovisAsp>
