   <extension
         point="org.eclipse.ui.views">
      <view
            name="Clovis Workspace"
            icon="icons/sample.gif"
            category="com.clovis.cw.bootmanager"
            class="com.clovis.cw.workspace.ClovisNavigator"
            id="com.clovis.cw.workspace.clovisWorkspaceView">
      </view>
       <category
            name="Clovis"
            id="com.clovis.cw.bootmanager">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Model Problems"
            icon="icons/validate_report.gif"
            category="com.clovis.cw.bootmanager"
            class="com.clovis.cw.workspace.ProblemsView"
            id="com.clovis.cw.workspace.problemsView">
      </view>
       <view
