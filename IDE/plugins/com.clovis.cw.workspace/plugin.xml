   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            objectClass="org.eclipse.core.resources.IProject"
            class="com.clovis.cw.workspace.project.CwProjectPropertyPage"
            name="Clovis System Project"
            id="com.clovis.cw.workspace.project.CwProjectPropertyPage"
      		adaptable="true">
      		<filter
               name="projectNature"
               value="com.clovis.cw.workspace.natures.SystemProjectNature">
         	</filter>
      </page>      
   </extension>
   <extension
         id="builders.ClovisBuilder"
         name="Clovis Builder"
         point="org.eclipse.core.resources.builders">

      <builder isConfigurable="true">
      	<run class="com.clovis.cw.workspace.builders.ClovisBuilder">
      	  <parameter name="optimize" value="true"/>
      	</run>
      </builder>
   </extension>
   <extension
         id="natures.SystemProjectNature"
         name="System Project Nature"
         point="org.eclipse.core.resources.natures">
	  <runtime>	
      	<run class="com.clovis.cw.workspace.natures.SystemProjectNature">
      	  <parameter name="optimize" value="true"/>
      	</run>
      </runtime>		
   </extension>
   <extension
         id="builders.ApplicationSourceBuilder"
         name="Application Source Builder"
         point="org.eclipse.core.resources.builders">

      <builder isConfigurable="true">
      	<run class="com.clovis.cw.workspace.builders.ApplicationSourceBuilder">
      	  <parameter name="optimize" value="true"/>
      	</run>
      </builder>
   </extension>
   <extension
         id="natures.ApplicationSourceNature"
         name="System Project Nature"
         point="org.eclipse.core.resources.natures">
	  <runtime>	
      	<run class="com.clovis.cw.workspace.natures.ApplicationSourceNature">
      	  <parameter name="optimize" value="true"/>
      	</run>
      </runtime>		
