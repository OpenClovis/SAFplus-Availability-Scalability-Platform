   <extension point="org.eclipse.ui.perspectives">
      <perspective name="OpenClovis IDE"
            icon="icons/cw_persp.gif"
            class="com.clovis.cw.workspace.project.CWPerspectiveFactory"
            id="com.clovis.cw.ui.CWPerspective">
      </perspective>
   </extension>
  
   <extension point="org.eclipse.ui.newWizards">
      <category
            name="Clovis"
            id="com.clovis.cw.workspace">
      </category>
      <wizard name="Clovis System Project"
            icon="icons/cw_persp.gif"
            category="com.clovis.cw.workspace"
            class="com.clovis.cw.workspace.project.ClovisProjectCreationWizard"
            project="true"
            finalPerspective="com.clovis.cw.ui.CWPerspective"
            id="com.clovis.cw.ui.wizards.simple">
         <description>
             Create a Clovis System Project
         </description>
      </wizard>
