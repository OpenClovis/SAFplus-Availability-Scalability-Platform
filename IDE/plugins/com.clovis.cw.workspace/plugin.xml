<extension point="org.eclipse.ui.ide.resourceFilters">
<filter
      selected="true"
      pattern="configs"/>
<filter
      selected="true"
      pattern="models"/>      
</extension> 

     <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
