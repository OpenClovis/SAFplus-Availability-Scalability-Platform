<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="com.clovis.cw.genericeditor"
   name="OpenClovis Genericeditor Plug-in"
   version="3.0"
   provider-name="OpenClovis"
   class="com.clovis.cw.genericeditor.GenericeditorPlugin">

   <runtime>
      <library name="com.clovis.cw.genericeditor.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.gef"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="com.clovis.common.utils"/>
      <import plugin="com.clovis.cw.data"/>
   </requires>

</plugin>
