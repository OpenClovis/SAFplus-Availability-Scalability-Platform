<?xml version="1.0"?>
<MenuBars>
    <Name>EditorMenubars</Name>
	<MenuBar>
    <Name>EditorMenubar</Name>
	<Menu>
       	<Name>Properties</Name>
       	<Label>Properties...</Label>
       	<Hint>Properties Dialog</Hint>
       	<Icon>iconaas</Icon>
       	<Default>true</Default>
       	<ActionClass>
           <ClassName>com.clovis.cw.editor.ca.dialog.ComponentPropertiesAction</ClassName>
           <CallOrder>2</CallOrder>
           <CallType>INORDER</CallType>
           <Argument>shell</Argument>
           <Argument>selection</Argument>
           <ConstantArgument>adsd</ConstantArgument>
       	</ActionClass>
		<DisplayOrder>1</DisplayOrder>
		<Separator>true</Separator>
	</Menu>
	<Menu>
       	<Name>SNMP Properties</Name>
       	<Label>Sub Agent Properties...</Label>
       	<Hint>SNMP Code Gen Properties Dialog</Hint>
       	<Icon>iconaas</Icon>
       	<Default>false</Default>
       	<ActionClass>
           <ClassName>com.clovis.cw.editor.ca.action.SNMPCodeGenPropertiesAction</ClassName>
           <CallOrder>2</CallOrder>
           <CallType>INORDER</CallType>
           <Argument>shell</Argument>
           <Argument>selection</Argument>
           <ConstantArgument>adsd</ConstantArgument>
       	</ActionClass>
		<DisplayOrder>1</DisplayOrder>
		<Separator>true</Separator>
	</Menu>
	<Menu>
       	<Name>Associate Resources</Name>
       	<Label>Associate Resources...</Label>
       	<Hint>Associate Resources Dialog</Hint>
       	<Icon>iconaas</Icon>
       	<ActionClass>
           <ClassName>com.clovis.cw.editor.ca.dialog.AssociateResourceAction</ClassName>
           <CallOrder>2</CallOrder>
           <CallType>INORDER</CallType>
           <Argument>shell</Argument>
           <Argument>selection</Argument>
           <ConstantArgument>adsd</ConstantArgument>
       	</ActionClass>
		<DisplayOrder>1</DisplayOrder>
		<Separator>true</Separator>
	</Menu>
	<Menu>
       	<Name>Add New Node</Name>
       	<Label>Add New Node...</Label>
       	<Hint>Adds a new node to the component editor model</Hint>
       	<Icon>iconaas</Icon>
       	<ActionClass>
           <ClassName>com.clovis.cw.workspace:com.clovis.cw.workspace.action.AddNewNodeAction</ClassName>
           <CallOrder>2</CallOrder>
           <CallType>INORDER</CallType>
           <Argument>shell</Argument>
           <Argument>project</Argument>
           <ConstantArgument>adsd</ConstantArgument>
       	</ActionClass>
		<DisplayOrder>1</DisplayOrder>
		<Separator>true</Separator>
	</Menu>
	<Menu>
       	<Name>CreateModelTemplate</Name>
       	<Label>Create Model Template...</Label>
       	<Hint>Creates a model template from the classes of the editor</Hint>
        <Icon>iconaas</Icon>
       	<Default>false</Default>
       	<ActionClass>
           <ClassName>com.clovis.cw.workspace:com.clovis.cw.workspace.modelTemplate.CreateComponentModelTemplateAction</ClassName>
           <CallOrder>2</CallOrder>
           <Argument>shell</Argument>
           <Argument>selection</Argument>
           <ConstantArgument>adsd</ConstantArgument>
       	</ActionClass>
		<DisplayOrder>1</DisplayOrder>
		<Separator>true</Separator>
	</Menu>
	</MenuBar>  
