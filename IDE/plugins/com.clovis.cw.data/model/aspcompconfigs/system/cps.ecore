        <details key="kind" value="element"/>
      </eAnnotations>
      <eAnnotations source="CWAnnotation">
        <details key="label" value="Preference"/>
        <details key="tooltip" value="Priority of Node"/>
      </eAnnotations>
    </eStructuralFeatures>
