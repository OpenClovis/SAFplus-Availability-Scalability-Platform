        <details key="label" value="Checkpoint"/>
        <details key="tooltip" value="Create CheckPoints"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CheckPointType">
