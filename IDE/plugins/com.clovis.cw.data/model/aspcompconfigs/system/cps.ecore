        <details key="kind" value="element"/>
      </eAnnotations>
      <eAnnotations source="CWAnnotation">
        <details key="label" value="Number of Maximum Replicas"/>
        <details key="tooltip" value="Maximum Replicas of corresponding checkpoint"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="replicaPolicy" eType="#//PolicyEnum"
