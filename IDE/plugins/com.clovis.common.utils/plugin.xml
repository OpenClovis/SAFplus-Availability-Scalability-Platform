<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="com.clovis.common.utils"
   name="OpenClovis Java Utils Plug-in"
   version="3.0"
   provider-name="OpenClovis"
   class="com.clovis.common.utils.UtilsPlugin">

   <runtime>
      <library name="com.clovis.common.utils.jar">
         <export name="*"/>
      </library>
      <library name="lib/log4j/log4j-1.2.9.jar">
         <export name="*"/>
      </library>
      <library name="lib/castor/castor-0.9.6.jar">
         <export name="*"/>
      </library>
      <library name="lib/castor/xercesImpl.jar">
         <export name="*"/>
      </library>
      <library name="lib/castor/castor-0.9.5.3-anttaskdef.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-logging/commons-logging-api.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.gef"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.ui.ide"/>
   </requires>
</plugin>
