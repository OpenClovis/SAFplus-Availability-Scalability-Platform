<?xml version='1.0'?>
<project name="Build" default="all">
<!-- Build -->	
<target name="build">
	<exec dir="${makearea.loc}" executable="${script.file}" failonerror="true"/>
</target>
<!--
Target to make images.
-->
<target name="all"
    depends="build"/>
    <echo>Make image started for ${project.name}</echo>
</project>
